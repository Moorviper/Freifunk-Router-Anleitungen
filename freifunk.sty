\newcommand{\monocodebox}[6]{

\begin{mintedbox}[breakable=#4,
%ribbonbox,
 bottomrule=0.5mm,
 width=\paperwidth-3cm,
 boxsep=1mm, 
 enhanced=true,
 colframe = monoblack,
 drop fuzzy shadow,
 colback = black
 ]{#2}%
 

     \inputminted[firstline=#5,lastline=#6, 
     linenos=true, framesep=2mm, mathescape, numbersep=5pt,tabsize=4,formatcom=\color{white},%fontsize=\footnotesize, 
]{#1}{#3}%

\end{mintedbox}%
\vspace{+0.2cm}

}

\newcommand{\codebox}[6]{
  \begin{mintedbox}[breakable=#4,
%ribbonbox,
 bottomrule=1mm,
 width=\paperwidth-3cm,
 boxsep=1mm, 
 enhanced=true,
 colframe=black!50!white,
 drop fuzzy shadow,
 %colback = white!10
 ]{#2}%
 
    \inputminted[firstline=#5,lastline=#6, 
     linenos=true, framesep=2mm, mathescape, numbersep=5pt,tabsize=2,%fontsize=\footnotesize, 
]{#1}{#3}%


\end{mintedbox}%
}

